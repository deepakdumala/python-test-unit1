# Tuple assignment
x, y = 10, 5

# Print values before expression
print("Before expression:")
print("x value:", x)
print("y value:", y)

# Effect of operator precedence in an expression
x, y = y + 2, x - 3 * y

# Print values after expression
print("\nAfter expression:")
print("x value:", x)
print("y value:", y)
